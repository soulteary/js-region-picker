{
  "name": "js-region-picker",
  "version": "1.0.0",
  "private": true,
  "description": "A JavaScript Region Range Picker written by vanilla JS.",
  "scripts": {
    "build:desktop": "npx google-closure-compiler --js ../assets/js/region-picker.js --language_out=ECMASCRIPT5 --js_output_file ../assets/js/region-picker.min.js --isolation_mode=IIFE",
    "build:mobile": "npx google-closure-compiler --js ../assets/js/region-picker.mobile.js --language_out=ECMASCRIPT5 --js_output_file ../assets/js/region-picker.mobile.min.js --isolation_mode=IIFE",
    "build": "npm run build:desktop && npm run build:mobile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soulteary/js-year-picker.git"
  },
  "bugs": {
    "url": "https://github.com/soulteary/js-year-picker/issues"
  },
  "homepage": "https://github.com/soulteary/js-year-picker#readme",
  "devDependencies": {
    "google-closure-compiler": "^20230502.0.0"
  }
}